@model MVC_CV.Models.Entity.Tbl_Education
@{
    ViewBag.Title = "UpdateEducation";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h4> Update Existed Education Informations</h4>
<br />

<form method="post">
    @Html.Label("Title")
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.Title, "", new { @style = "color:red" })
    <br />
    @Html.Label("Sub-Title")
    @Html.TextBoxFor(x => x.SubTitle1, new { @class = "form-control" })
    <br />
    @Html.Label("Sub-Title 2")
    @Html.TextBoxFor(x => x.SubTitle2, new { @class = "form-control" })
    <br />
    @Html.Label("Grade Point Average (GPA)")
    @Html.TextBoxFor(x => x.GPA, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.GPA, "", new { @style = "color:blue" })
    <br />
    @Html.Label("Date")
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    <br />

    <button class="btn btn-outline-primary  "> Update</button>
</form>



