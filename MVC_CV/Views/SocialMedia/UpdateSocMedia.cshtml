@model MVC_CV.Models.Entity.Tbl_SocialMedia

@{
    ViewBag.Title = "UpdateSocMedia";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h4> Edit Social Media</h4>
<br />

<form method="post">
    @Html.Label("Social Media Name")
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Font Awoseme Icon Code")
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
    <br />

    <button class="btn btn-info "> Update</button>
</form>


